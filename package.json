  "license": "MIT",
  "copyright": "(c) The C2 Group (c2experience.com)",
  "bugs": {
    "url": "https://github.com/TheC2Group/vanilla-js-modal/issues"
  },
  "homepage": "https://github.com/TheC2Group/vanilla-js-modal",
  "dependencies": {
    "c2-event-handler": "^2.5.2",
    "gulp": "^4.0.2",
    "gulp-run": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "acorn": "^6.0.0",
    "browserify": "^16.2.3",
    "rollup": "^1.12.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0"
  }
