{
  "name": "vanilla-js-modal",
  "version": "1.0.0",
  "description": "Add accessible modals to a web page using vanilla JS.",
  "jsnext:main": "modal-handler.js",
  "main": "cjs/modal-handler.js",
  "export": "MODAL",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "build": "rollup -c rollup.config.js",
    "bundle-example": "browserify example/js/simple.js -o example/bundle/simple.js && browserify example/js/on-the-fly.js -o example/bundle/on-the-fly.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:TheC2Group/vanilla-js-modal.git"
  },
  "keywords": [
    "modal"
  ],
